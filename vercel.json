{
  "version": 2,
  "name": "agenda-medicao-backend",
  "builds": [
    { 
      "src": "server.js", 
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    { 
      "src": "/api/(.*)", 
      "dest": "/server.js" 
    },
    { 
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))", 
      "dest": "/$1" 
    },
    { 
      "src": "/(.*)", 
      "dest": "/server.js" 
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
